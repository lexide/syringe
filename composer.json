{
  "name": "lexide/syringe",
  "description": "Lexide Syringe, configuration utility for Pimple",
  "license": "MIT",
  "homepage": "https://github.com/lexide/syringe",
  "require": {
    "php": ">=7.3",
    "pimple/pimple": "^3.0.0",
    "symfony/yaml": "^3.0.0 | ^4.0.0 | ^5.0.0",
    "psr/log": "^1.0.1 || ^2.0 || ^3.0",
    "ext-mbstring": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0.0",
    "mockery/mockery": "^1.0.0",
    "mikey179/vfsstream": "^1.6.0",
    "symfony/console": "^5.0.0"
  },
  "autoload": {
    "psr-4": {
      "Lexide\\Syringe\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Lexide\\Syringe\\Test\\": "tests"
    }
  },
  "suggest": {
    "ext-yaml": "Allows much faster parsing of Yaml files"
  }
}
