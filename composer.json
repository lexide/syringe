{
  "name": "lexide/syringe",
  "description": "Lexide Syringe, configuration utility for Pimple",
  "license": "MIT",
  "homepage": "https://github.com/lexide/syringe",
  "require": {
    "php": ">=5.4.0",
    "pimple/pimple": "~3.0",
    "symfony/yaml": "~2.6|~3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4",
    "mikey179/vfsstream": "~1.6.7"
  },
  "autoload": {
    "psr-4": {
      "Lexide\\Syringe\\": "src",
      "Lexide\\Syringe\\Tests\\": "tests/unit",
      "Lexide\\Syringe\\IntegrationTests\\": "tests/integration"
    }
  },
  "suggest": {
    "ext-yaml": "Allows much faster parsing of Yaml files"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
